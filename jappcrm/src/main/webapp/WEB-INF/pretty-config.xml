<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.0 
	http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.0.xsd">
	<url-mapping id="logout">
		<pattern value="/logout" />
		<view-id value="/faces/home.jsf" />
		<action>#{authController.logout}</action>
	</url-mapping>

	<url-mapping id="crmReport">
		<pattern
			value="/crms/#{crmReportController.crmNodeId}/reports/#{crmReportController.reportNodeId}" />
		<view-id value="/faces/crmReport/crmReport.jsf" />
	</url-mapping>

	<url-mapping id="crmField">
		<pattern value="/admin/crmFields/#{crmFieldController.nodeId}" />
		<view-id value="/faces/admin/crmField/update.jsf" />
	</url-mapping>

	<url-mapping id="communicateeUpdate">
		<pattern value="/communicatees/#{communicateeController.nodeId}" />
		<view-id value="/faces/communicatee/update.jsf" />
	</url-mapping>

	<url-mapping id="corporationTypeUpdate">
		<pattern value="/corporationTypes/#{corporationTypeController.nodeId}" />
		<view-id value="/faces/corporationType/update.jsf" />
	</url-mapping>

	<url-mapping id="certificateUpdate">
		<pattern value="/certificates/#{certificateController.nodeId}" />
		<view-id value="/faces/certificate/update.jsf" />
	</url-mapping>

	<url-mapping id="indicatorValueUpdate">
		<pattern value="/indicatorValues/#{indicatorValueController.nodeId}" />
		<view-id value="/faces/indicatorValue/update.jsf" />
	</url-mapping>

	<url-mapping id="customerUpdate">
		<pattern value="/customers/#{customerController.nodeId}" />
		<view-id value="/faces/customer/update.jsf" />
	</url-mapping>

	<url-mapping id="customerIndicatorUpdate">
		<pattern value="/customer/indicators/#{customerIndicatorController.nodeId}" />
		<view-id value="/faces/customer/indicator/update.jsf" />
	</url-mapping>

	<url-mapping id="customerReportUpdate">
		<pattern value="/customer/reports/#{customerReportController.nodeId}" />
		<view-id value="/faces/customer/report/update.jsf" />
	</url-mapping>

	<url-mapping id="storageUpdate">
		<pattern value="/storages/#{storageController.nodeId}" />
		<view-id value="/faces/storage/update.jsf" />
	</url-mapping>

	<url-mapping id="storageIndicatorUpdate">
		<pattern value="/storage/indicators/#{storageIndicatorController.nodeId}" />
		<view-id value="/faces/storage/indicator/update.jsf" />
	</url-mapping>

	<url-mapping id="storageReportUpdate">
		<pattern value="/storage/reports/#{storageReportController.nodeId}" />
		<view-id value="/faces/storage/report/update.jsf" />
	</url-mapping>
</pretty-config>
