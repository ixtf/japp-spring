<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.0 
	http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.0.xsd">
	<url-mapping id="logout">
		<pattern value="/logout" />
		<view-id value="/faces/home.jsf" />
		<action>#{authController.logout}</action>
	</url-mapping>

	<url-mapping id="communicateeUpdate">
		<pattern value="/communicatees/#{communicateeController.nodeId}" />
		<view-id value="/faces/communicatee/update.jsf" />
	</url-mapping>

	<url-mapping id="crmTypeUpdate">
		<pattern value="/crmTypes/#{crmTypeController.nodeId}" />
		<view-id value="/faces/crmType/update.jsf" />
	</url-mapping>

	<url-mapping id="indicatorValueUpdate">
		<pattern value="/indicatorValues/#{indicatorValueController.nodeId}" />
		<view-id value="/faces/indicatorValue/update.jsf" />
	</url-mapping>

	<url-mapping id="customerUpdate">
		<pattern value="/customers/#{customerController.nodeId}" />
		<view-id value="/faces/customer/update.jsf" />
	</url-mapping>

	<url-mapping id="customerBasicInfoReport">
		<pattern
			value="/customers/#{customerReportController.nodeId}/basicInfoReport" />
		<view-id value="/faces/customer/basicInfoReport.jsf" />
	</url-mapping>

	<url-mapping id="customerCreditRiskReport">
		<pattern
			value="/customers/#{customerReportController.nodeId}/creditRiskReport" />
		<view-id value="/faces/customer/creditRiskReport.jsf" />
	</url-mapping>

	<url-mapping id="customerFiCreditRiskReport">
		<pattern
			value="/customers/#{customerReportController.nodeId}/fiCreditRiskReport" />
		<view-id value="/faces/customer/fiCreditRiskReport.jsf" />
	</url-mapping>

	<url-mapping id="storageUpdate">
		<pattern value="/storages/#{storageController.uuid}" />
		<view-id value="/faces/storage/update.jsf" />
	</url-mapping>

	<url-mapping id="storageCreditRatingReport">
		<pattern
			value="/storages/#{storageReportController.nodeId}/creditRatingReport" />
		<view-id value="/faces/storage/creditRatingReport.jsf" />
	</url-mapping>

	<url-mapping id="storageFiCreditRiskReport">
		<pattern
			value="/storages/#{storageReportController.nodeId}/fiCreditRiskReport" />
		<view-id value="/faces/storage/fiCreditRiskReport.jsf" />
	</url-mapping>

	<!-- <url-mapping id="indicatorNew"> <pattern value="/#{indicatorController.type}/indicator" 
		/> <view-id value="/faces/indicator/update.jsf" /> </url-mapping> <url-mapping 
		id="indicatorUpdate"> <pattern value="/#{indicatorController.type}/indicator/#{indicatorController.nodeId}" 
		/> <view-id value="/faces/indicator/update.jsf" /> </url-mapping> -->

	<url-mapping id="customerIndicatorUpdate">
		<pattern value="/customer/indicators/#{customerIndicatorController.nodeId}" />
		<view-id value="/faces/customer/indicator/update.jsf" />
	</url-mapping>

	<url-mapping id="storageIndicatorUpdate">
		<pattern value="/storage/indicators/#{storageIndicatorController.nodeId}" />
		<view-id value="/faces/storage/indicator/update.jsf" />
	</url-mapping>
</pretty-config>
