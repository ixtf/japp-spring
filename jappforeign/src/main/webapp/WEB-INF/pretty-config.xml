<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.0 
	http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.0.xsd">
	<url-mapping id="home">
		<pattern value="/" />
		<view-id value="/faces/home.jsf" />
	</url-mapping>

	<url-mapping id="vbakManage">
		<pattern value="/vbakManage" />
		<view-id value="/faces/vbak/open.jsf" />
	</url-mapping>
	<url-mapping id="vbakUpdate">
		<pattern value="/vbak/#{vbeln:vbakController.vbeln}" />
		<view-id value="/faces/vbak/update.jsf" />
		<action>#{vbakController.openVbak}</action>
	</url-mapping>

